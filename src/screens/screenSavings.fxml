<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="463.0" prefWidth="276.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.createSavingsAccountController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Savings Account: " />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#generateAccountNumber" text="Generate Account Number" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="savingAccNumLabel" text="Account Number: " />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="savingAccNumLabel1" text="Your Name:   " />
            <TextField />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="166.0" prefWidth="276.0">
         <children>
            <Button mnemonicParsing="false" text="Done">
               <HBox.margin>
                  <Insets bottom="1.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
